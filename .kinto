version: 0.0.1
kinto:
  type: BACKEND_API
  build:
    language: DOCKERFILE
    subfolderPath: .
    dockerfileName: Dockerfile
  deploy:
    memoryMB: 512
    cpuCores: -1
    costOptimization: true
    sleepMode: true
    protocol: HTTP
    port: 8080
    environment:
      PATATE: 8000
    deployTimeoutSeconds: 60